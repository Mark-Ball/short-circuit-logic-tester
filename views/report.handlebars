<h1>Report</h1>

<div>
    <p>Score: {{ quizResponse.score }}/10</p>
</div>
<div>
    <p>Time: {{ quizResponse.time }}s</p>
</div>

<table>
    <thead>
        <tr>
            <th>Question</th>
            <th>Response</th>
            <th>Answer</th>
        </tr>
    </thead>
    <tbody>
        {{#each quizResponse.questions}}
            <tr>
                <td>{{this.text}}</td>
                <td>{{this.response}}</td>
                <td>{{this.answer}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>